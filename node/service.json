{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "status": {
      "path": "/_v/getClientsPoints",
      "public": true
    }
  },
  "events": {
    "allStates": {
      "sender": "vtex.orders-broadcast",
      "topics": [
        "ready-for-handling"
      ]
    },
    "readyForHandlingEvent": {
      "sender": "vtex.orders-broadcast",
      "topics": [
        "order-created",
        "ready-for-handling"
      ]
    }
  }
}
